{
  "name": "tiny-markdown-editor",
  "version": "0.2.1",
  "description": "TinyMDE: A tiny, ultra low dependency, embeddable HTML/JavaScript Markdown editor.",
  "main": "lib/index.js",
  "files": [
    "dist/*",
    "lib/*",
    "lib/**/*.d.ts"
  ],
  "types": "lib/index.d.ts",
  "keywords": [
    "embeddable",
    "markdown",
    "editor"
  ],
  "scripts": {
    "test": "npx gulp && npx jest",
    "test:chromium": "npx gulp && npx jest --selectProjects chromium",
    "test:firefox": "npx gulp && npx jest --selectProjects firefox",
    "test:webkit": "npx gulp && npx jest --selectProjects webkit",
    "test:single": "npx gulp test",
    "build": "npx gulp",
    "dev": "npx gulp dev",
    "typecheck": "npx tsc --noEmit",
    "prepublishOnly": "npx gulp prepublish",
    "releasePatch": "npx gulp releasePatch",
    "release": "npx gulp release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefago/tiny-markdown-editor.git"
  },
  "author": "Jens-Fabian Goetzmann <mail@jefago.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jefago/tiny-markdown-editor/issues"
  },
  "type": "commonjs",
  "homepage": "https://github.com/jefago/tiny-markdown-editor#readme",
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "core-js": "^3.42.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@octokit/rest": "^22.0.0",
    "@playwright/test": "^1.54.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@rollup/stream": "^3.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "del": "^8.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "express": "^5.1.0",
    "gulp": "^5.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-size": "^5.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "playwright": "^1.54.1",
    "postcss-import": "^16.1.0",
    "rollup": "^4.41.1",
    "rollup-plugin-eslint": "^7.0.0",
    "terser": "^5.40.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
